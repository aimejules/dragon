<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <report
                id="wolf_team_report"
                model="wolf.team"
                string="ReportTitle"
                name="wolf_training.team_report_teamplate"
                file="wolf_training.team_report_teamplate"
                report_type="qweb-pdf"/>

        <template id="team_report_teamplate">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="doc">
                    <t t-call="web.external_layout">
                        <div class="page">
                            <h3>Mon titre</h3>
                            <table class="table table-condensed">
                                <tr>
                                    <td>Nom</td>
                                    <td>
                                        <span t-field="doc.name"/>
                                    </td>
                                </tr>
                            </table>

                        </div>
                    </t>
                </t>
            </t>
        </template>

    </data>
</odoo>